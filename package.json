{
  "name": "digi-wallet-sdk",
  "version": "1.0.2",
  "description": "DigiWallet SDK for NodeJS",
  "main": "./lib/index.js",
  "scripts": {
    "clean": "rimraf lib",
    "test": "npm run lint && npm run cover",
    "test:prod": "cross-env BABEL_ENV=production npm run test",
    "test:only": "mocha",
    "test:watch": "npm test -- --watch",
    "test:examples": "node examples/",
    "cover": "nyc --check-coverage npm run test:only",
    "lint": "eslint src test",
    "build": "cross-env BABEL_ENV=production babel src --out-dir lib",
    "prepublish": "npm run clean && npm run lint && npm run build && npm run test"
  },
  "files": [
    "lib",
    "src"
  ],
  "keywords": [
    "digiwallet",
    "digi-wallet",
    "sdk"
  ],
  "author": "Petar Lazarevic <petar.lazarevic23@gmail.com>",
  "license": "ISC",
  "directories": {
    "example": "examples",
    "lib": "lib",
    "test": "test"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-add-module-exports": "^1.0.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-minify": "^0.5.0",
    "chai": "^4.1.2",
    "cross-env": "^5.1.3",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "mocha": "^7.1.1",
    "nyc": "^15.0.0",
    "rimraf": "^2.6.2"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "md5": "^2.2.1",
    "xml2js": "^0.4.23"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Hogar23/digi-wallet-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/Hogar23/digi-wallet-sdk/issues"
  },
  "homepage": "https://github.com/Hogar23/digi-wallet-sdk#readme"
}
